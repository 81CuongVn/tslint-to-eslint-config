{
    "editor.tabSize": 4,
    "eslint.rules.customizations": [{ "rule": "*", "severity": "warn" }],
    "files.exclude": {
        "**/*.d.ts": true,
        "**/*.js.map": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    },
    "prettier.ignorePath": ".prettierignore",
    "typescript.tsdk": "node_modules/typescript/lib"
}
